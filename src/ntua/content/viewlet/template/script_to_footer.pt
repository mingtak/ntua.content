<div class="script_to_footer" style="display:none">

        <!-- colorbox -->
        <script src="/ntua/++theme++ntua.theme/js/colorbox-master/jquery.colorbox-min.js"></script>

    <script>
      // 成員介紹colorbox
      $(document).ready(function(){
        if($(window).width() > 800){
            $(".ajax").colorbox({width:"40%"});
        }else{
            $(".ajax").colorbox({width:"90%"});
        };

        if($(window).width() > 800){
            $(".iframe").colorbox({iframe:true, width:"40%", height:"80%"});
        }else{
            $(".iframe").colorbox({iframe:true, width:"90%", height:"90%"});
        };

      });


      // 回上一頁
      $(".return_page").click(function(){
        window.history.go(-1);
      });

    </script>

