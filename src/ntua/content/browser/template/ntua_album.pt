            <a tal:attributes="href string:${item/absolute_url}/@@images/image/large; title item/title"
               data-rel="prettyPhoto[gallery2]" class="zoom"><i class="fa fa-search"></i></a>

          </div> 
      </div>
  </div>
</tal:images>











            </div>
           </div>
         </div>
        </div>
       </div>
      </section>
     </div>
        <!--Content Wrap End-->

</metal:content-core>
